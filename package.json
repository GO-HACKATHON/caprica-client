  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.5.0",
    "firebase": "^3.7.3",
    "react": "~15.4.0",
    "react-native": "0.42.3",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "react/forbid-component-props": "off",
      "unicorn/filename-case": "off"
    }
