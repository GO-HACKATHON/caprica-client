{
  "name": "caprica-client",
  "version": "1.3.0",
  "private": true,
  "devDependencies": {
    "eslint-config-xo": "^0.18.1",
    "eslint-config-xo-react": "^0.11.1",
    "eslint-plugin-react": "^6.10.3",
    "jest-expo": "^0.3.0",
    "react-test-renderer": "~15.4.1"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
